          <li role="presentation"><a href="#">Growth</a></li>
          <li role="presentation"><a href="#">Sales/BD</a></li>
      </ul><br />

      <div class="panel panel-info">
        <div class="panel-heading">
          <h4>Available Mentor Slots <small>Pick a slot, click Book Now, fill out the form and then click Send Invite</small></h4> 
        </div>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Day/Time</th>
              <th>Book this slot</th>
              <!-- <th>Calendar</th> -->
            </tr>
          </thead>
          <tbody>
            {{#each slots}}
            <tr>
                <td>
                  {{this}}
                </td>
              <td><a href="#" class="btn btn-primary btn-xs" role="button">Book now</a></td>
              <!-- <td><a href="#" class="btn btn-danger btn-xs" role="button">Send Invite</a></td> -->
            </tr>
            {{/each}}
<!--             <tr>
              <th>Thurs, May 7th at 1:30PM</th>
              <td><a href="#" class="btn btn-primary btn-xs" role="button">Book now</a></td>
              <td><a href="#" class="btn btn-danger btn-xs" role="button">Send Invite</a></td>
            </tr>
            <tr>
              <th>Mon, May 11th at 5:30PM</th>
              <td><a href="#" class="btn btn-primary btn-xs" role="button">Book now</a></td>
              <td><a href="#" class="btn btn-danger btn-xs" role="button">Send Invite</a></td>
            </tr> -->
          </tbody>
        </table>
      </div><br />

      <div class="panel panel-success">
        <div class="panel-heading">
          <h4>Upcoming Mentors <small>View and book available mentor slots</small></h4> 
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Date</th>
              <th>Name</th>
              <th>Title</th>
              <th>Company</th>
              <th>Owner</th>
            </tr>
          </thead>
          <tbody>
            {{#each upcoming}}
            <tr>
              <th>{{datetime}}</th>
              <a href="{{link}}"> <td>{{engmentorname}}</td></a>
              <td>blah</td>
              <td>blah</td>
              <td>{{student}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div><br />

      <div class="panel panel-warning">
        <div class="panel-heading">
          <h4>Past Mentors <small>Previous mentors, info and links to audio</small></h4>
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Date</th>
              <th>Name</th>
              <th>Title</th>
              <th>Company</th>
              <th>Owner</th>
            </tr>
          </thead>
          <tbody>
            {{#each past}}
            <tr>
              <th>{{datetime}}</th>
              <td>{{engmentorname}}</td>
              <td>blah</td>
              <td>blah</td>
              <td>{{student}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
