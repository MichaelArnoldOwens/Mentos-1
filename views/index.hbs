    <!-- Wrap all page content here -->
    <div id="wrap">
      <!-- Fixed navbar -->
      <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/mentors">Tradecraft Mentors</a>
          </div>
          <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
              <li class="active"><a href="#">Home</a></li>
              <li><a href="mailto:troy@tradecrafted.com?subject=Feedback/Sggestions&amp;body=How can we improve this site? Tell us below:">Suggestions?</a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
      
    <div class="container">
      <div class="blockquote">
        <blockquote>
        <p>View upcoming mentors, past mentors, and schedule new ones, all from here.</p>
        </blockquote>
      </div><br />

      <ul class="nav nav-pills">
          <li id="allTrackTab" role="presentation"><a href="/mentors">All-track</a></li>
          <li id="engTab" role="presentation"><a href="/mentors/eng">Engineering</a></li>
          <li id="pdTab" role="presentation"><a href="/mentors/pd">UX/Product Design</a></li>
          <li id="growthTab" role="presentation"><a href="/mentors/growth">Growth</a></li>
          <li id="salesTab" role="presentation"><a href="/mentors/sales">Sales/BD</a></li>
      </ul><br /><br />

      <div class="panel panel-info">
        <div class="panel-heading">
          <h4>Upcoming Mentors <small>Upcoming mentors scheduled to speak</small></h4> 
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Date</th>
              <th>Name</th>
              <th>Title, Company</th>
              <th>Student</th>
              <th>Add to gCal</th>
            </tr>
          </thead>
          <tbody>
            {{#each upcoming}}
            <tr>
              <th>{{datetime}}</th>
              <td><a href="{{linkedin}}">{{mentorname}}</a></td>
              <td>{{titlecompany}}</td>
              <td>{{student}}</td>
              <td><a href="https://www.google.com/calendar/render?action=TEMPLATE&text=Mentor%20Talk:%20{{mentorname}}&dates={{datetimeCalendar}}&location=855%20Front%20St%20San%20Francisco&sprop=name:Name&sprop=website:EventWebite&details=Who:%20{{mentorname}}%20-%20{{titlecompany}}%0A%0ADetails:%20{{tellusaboutyourmentor}}&sf=true&output=xml" target="_blank" class="btn btn-danger btn-xs" role="button">Create Event</a></td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div><br />

      <div class="panel panel-success">
        <div class="panel-heading">
          <h4>Open Mentor Slots <small>Book away!</small></h4> 
        </div>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Day/Time</th>
              <th>Book this slot</th>
            </tr>
          </thead>
          <tbody>
            {{#each slots}}
            <tr>
                <td>
                  {{this}}
                </td>
              <td><a id="formLink" class="btn btn-primary btn-xs" role="button" target="_blank">Book now</a></td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div><br />

      <div class="panel panel-warning">
        <div class="panel-heading">
          <h4>Past Mentors <small>Previous mentors, info and links to audio</small></h4>
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Date</th>
              <th>Name</th>
              <th>Title, Company</th>
              <th>Student</th>
            </tr>
          </thead>
          <tbody>
            {{#each past}}
            <tr>
              <th>{{datetime}}</th>
              <td><a href="{{linkedin}}">{{mentorname}}</a></td>
              <td>{{titlecompany}}</td>
              <td>{{student}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div><br />

    <hr>

    <div id="footer">
      <div class="container">
        <center><p class="text-muted credit">Made with &hearts; from TC Eng (you know, upstairs...in the cloud)</p></center>
      </div>
    </div>

